    "    plt.rc('legend', fontsize=SIZE+3)  # legend fontsize\n",
    "    #plt.rc('figure', titlesize=SIZE)  # # size of the figure title\n",
    "    plt.figure(figsize=(7,6))\n",
    "    plt.loglog(ks,ek_box,'r',label='Box')    \n",
    "    plt.loglog(ks,np.power(ks,-5/3),'k')\n",
